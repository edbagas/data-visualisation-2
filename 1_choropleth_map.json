{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Global Internet Usage Rate per 100",
  "width": 800,
  "height": 600,
  "data": {
    "url": "https://github.com/edbagas/data-visualisation-2/raw/main/data/custom.geo%20(3)%20(1).json",
    "format": {"type": "topojson", "feature": "custom.geo (3)"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://github.com/edbagas/data-visualisation-2/raw/main/data/gapminder_internet.csv"
        },
        "key": "country",
        "fields": ["internetuserate"]
      }
    },
    {"calculate": "datum.internetuserate", "as": "Internet Usage Rate per 100"}
  ],
  "projection": {"type": "equirectangular"},
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Internet Usage Rate per 100",
      "type": "quantitative",
      "scale": {"type": "band"}
    },
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "Country"},
      {"field": "Internet Usage Rate per 100", "type": "quantitative"}
    ]
  },
  "config": {}
}